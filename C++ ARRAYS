#include <iostream> 
using namespace std;

int main() {
	string NoxFavSongs[] = { "Ghost by Nelward", "CLUB SANDWICH by Joey Valence and Brae", "HAUNTED HOUSE by SNAKE POOL", "CHATTERING LACK OF COMMON SENSE by Ghost and Pals"};
	for (int i = 0; i < 4; i++)
		cout << NoxFavSongs[i] << endl;
	NoxFavSongs[1] = "bedroom community by glass beach";
	cout << NoxFavSongs[2] << endl;
	for (int i = 0; i < 4; i++)
		if (NoxFavSongs[i] == "Blinding Lights by The Weeknd")
			cout << "True" << endl;
		else
			cout << "False" << endl;
	for (int i = 0; i < 4; i++)
		cout << NoxFavSongs[i] << endl;


}
